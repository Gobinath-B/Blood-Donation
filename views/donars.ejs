<!DOCTYPE html>
<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    
    .container {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
    }
    
    .frame {
      flex: 0 0 calc(50% - 20px);
      margin-bottom: 20px;
      padding: 20px;
      background-color: white;
      border: 2px solid red;
      border-radius: 5px;
    }
    
    @media (max-width: 767px) {
      .frame {
        flex: 0 0 100%;
      }
    }
  </style>
</head>
<body>
  <div class="container">
    <div class="frame">
      <% if (data && Array.isArray(data)) { %>
        <ul>
          <% data.forEach(function(item) { %>
            <li>Name: <%= item.name %></li>
            <li>Blood Group: <%= item.bloodGroup %></li>
            <li>Location: <%= item.location %></li>
            <li>Phone Number: <%= item['phone-number'] %></li>
            <li>Birth Year: <%= item['birth-year'] %></li>
            <br />
          <% }); %>
        </ul>
      <% } else { %>
        <p>No data found.</p>
      <% } %>
    </div>
    
  </div>
</body>
</html>
